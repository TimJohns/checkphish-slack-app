  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": ">=4.2.0 < 13",
    "@slack/types": "^1.9.0",
    "@types/shelljs": "^0.8.8",
    "shelljs": "^0.8.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
